
@model login_signup.Models.User
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <title>ViewUsers</title>
</head>
<body>
    <div class="container" style="background-color:ghostwhite">
        <h2 align="center" style="font-family:'Buxton Sketch'">Your Profile</h2>
        <table class="table table-striped table-bordered">
            <thead>

                <tr>
                    <th>  Id  </th>
                    <th>  Fname  </th>
                    <th>  Lname</th>
                    <th>  Age</th>
                    <th>  Phone</th>

                </tr>
            </thead>
            <tbody>
               
                    <tr>
                        <td>@Model.Id</td>
                        <td>@Model.Fname</td>
                        <td>@Model.Lname</td>
                        <td>@Model.Age</td>
                        <td>@Model.Phone</td>
                        <td><a href="/Home/Update/@Model.Id">Edit</a></td>

                    </tr>

               
            </tbody>
        </table>
    </div>

    <script src="~/Scripts/bootstrap.js"></script>
</body>
</html>
